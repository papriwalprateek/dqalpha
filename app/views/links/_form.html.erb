
<%= form_for([@quest, @quest.links.build], :remote => true, :remote => true, :update => "links_list", :position => :top) do |f| %>
  
  <div class="link_field" id="link_field">
    <%= f.label :body %><br />
    <%= f.text_field :body %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
