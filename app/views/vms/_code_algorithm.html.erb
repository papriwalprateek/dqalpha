
<%if @pseudocode!=nil && @pseudocode!=[]%>
<div id="code" class="vm_results">
   
        <%begin%>
						<div style="font-family:BentoSans;color:#666;font-size:2em;">Implementation</div><br>
						
						<%@pseudocode.each do |l|%>
						<% if l.text.length > 1%>
                        <div id = "searchresults"><%=raw l%></div>
                        <br>
						<%end%>
                        <%end%>
		<%rescue%>
		<%end%>
        
 </div>
</hr>
 <%end%>
<%if @e_code && @e_code!=[]%>
<div class="vm_results" id ="code">
                        <%begin%>
                                <%@e_code.each do |t|%>
                                <b>Commited by <%=raw t.user%> </b></br>
                                <pre>
                                <div id = "searchresults"><%=t.value%></div>
                               	</pre>
                                <%end%>
                        <%rescue%>
                      
                        <%end%>
<hr>
</div>
<%end%>
 <%if @langcodes!=nil && @langcodes!=[]%>

<div id="code" class="vm_results">

<h2>Rosetta codes</h2>

    <%j=0%>
	<%while j < @langcodes.length%> 
	 <hr>
	    <%k = 0%>
	    <%while k < @langcodes[j].length%>
	         <%=raw @langcodes[j][k]%>
	         <%k = k + 1%>
	     <%end%>
	     <%j = j + 1%>
	<%end%>
	
	</div>
	<%end%>
