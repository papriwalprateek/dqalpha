<%if @wiki_definition%>
<div class="vm_results" id ="algo_description">
                        <%begin%>
                                <%@wiki_definition.each do |t|%>
                                <div id = "searchresults"><%=raw t%></div>
                                <br>
                                <%end%>
                        <%rescue%>
                        <%end%>
<hr>
</div>
<%end%>
<%if @math%>
<div id="math_analysis" class="vm_results">
        <%begin%>
                        <%@math.each do |l|%>
                        <div id = "searchresults"><%=raw l%></div>
                        <br>
                        <%end%>
        <%rescue%>
        <%end%>
</div>           
<%end%>
<%if (@algorithm||@cmplx)%>     
<div class="vm_results" id ="algo_examples">
         <%if @algorithm%>
         <%begin%>
         				<div name="algo"></div>
                        <%@algorithm.each do |l|%>
                        <div id = "searchresults"><%=raw l%></div>
                        <br>
                        <%end%>
         <%rescue%>
         <%end%>
         <%end%>
                
                <%if @cmplx != []%>
                <%begin%>
                                <div style="font-family:BentoSans;color:#666;font-size:2em;" name = "cmpx">Complexity</div><br>
                                <%@cmplx.each do |t|%>
                                <div id = "searchresults"><%=raw t%></div>
                                <br>
                                <%end%>
                <%rescue%>
                <%end%>
                <%end%>
                
</div>

<%end%>


<%if @related_algorithm %>
<div class="vm_results" id="algo_related">                        
        <%if @related_algorithm[0] != "see also"%>
		<%begin%>
						<div style="font-family:BentoSans;color:#666;font-size:1.6em;">Related:</div><br>
						<%@related_algorithm.each do |l|%>
                        <div id = "searchresults"><%=raw l%></div>
                        <br>
                        <%end%>
		<%rescue%>
		<%end%>
        <%end%>
        
      
</div>
<%end%>
<%if @related_algorithm %>
<div class="vm_results" id="misc">                        
  <div style="font-family:BentoSans;color:#666;font-size:1.6em;">Videos:</div><br>
        <%@embed_vid.each do |e|%>
        	<iframe width="460" height="250" src="//www.youtube.com/embed/<%=raw e%>" frameborder="0" allowfullscreen></iframe>
        	<br>
        	<br>
        <%end%> 
</div>
<%end%>



<%if @e_desc && @e_desc!=[]%>
<div class="vm_results" id ="algo_description">
                        <%begin%>
                                <%@e_desc.each do |t|%>
                                Commited by <%=raw User.find(t.user_id).name%> </br>
                                <div id = "searchresults"><%=raw t.value%></div>
                                <br>
                                <%end%>
                        <%rescue%>
                      
                        <%end%>
<hr>
</div>
<%end%>

<%if @e_ex && @e_ex!=[]%>
<div class="vm_results" id ="algo_examples">
                        <%begin%>
                                <%@e_ex.each do |t|%>
                                Commited by <%=raw User.find(t.user_id).name%> </br>
                                <div id = "searchresults"><%=raw t.value%></div>
                                <br>
                                <%end%>
                        <%rescue%>
                      
                        <%end%>
<hr>
</div>
<%end%>

<%if @e_code && @e_code!=[]%>
<div class="vm_results" id ="code">
                        <%begin%>
                                <%@e_code.each do |t|%>
                                Commited by <%=raw User.find(t.user_id).name%> </br>
                                <div id = "searchresults"><%=raw t.value%></div>
                                <br>
                                <%end%>
                        <%rescue%>
                      
                        <%end%>
<hr>
</div>
<%end%>